<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.travel">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_ic"
        android:label="@string/app_name"
        android:roundIcon="@drawable/app_ic"
        android:supportsRtl="true"
        android:theme="@style/Theme.Travel">
        <activity android:name=".BookData.Delete"></activity>
        <activity android:name=".TravelData.AddTripData" />
        <activity android:name=".BookData.BookDetails" />
        <activity
            android:name=".HomeList.Home"
            android:label="@string/title_activity_home"
            android:theme="@style/Theme.Travel.NoActionBar" />
        <activity android:name=".ShowList.ShowList" />
        <activity android:name=".Users.SignUp" />
        <activity android:name=".Users.SignIn" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>